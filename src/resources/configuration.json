{
    "primary_language": "en",
    "debugging_mode": true,
    "router": [
        {"path": "/", "page": "index"},
        {"path": "/auth/login", "page": "login"},
        {"path": "/auth/logout", "page": "logout"},

        {"path": "/cloud/accounts", "page": "accounts"},
        {"path": "/cloud/accounts/manage", "page": "manage_account"},

        {"path": "/cloud/applications/manage", "page": "manage_application"},
        {"path": "/cloud/applications", "page": "applications"},

        {"path": "/coa/authentication_requests", "page": "authentication_requests"},
        {"path": "/coa/authentication_requests/view", "page": "view_authentication_request"}
    ],
    "libraries": {
        "SecuredWebSessions": {
            "directory_name": "sws",
            "autoloader": "sws.php",
            "check_class_exists": true,
            "namespace": "sws",
            "main_class": "sws"
        },
        "IntellivoidAccounts": {
            "directory_name": "IntellivoidAccounts",
            "autoloader": "IntellivoidAccounts.php",
            "check_class_exists": true,
            "namespace": "IntellivoidAccounts",
            "main_class": "IntellivoidAccounts"
        },
        "msqg": {
            "directory_name": "msqg",
            "autoloader": "msqg.php",
            "check_class_exists": true,
            "namespace": "msqg",
            "main_class": "msqg"
        }
    },
    "runtime_scripts": {
        "initialize": [],
        "on_page_load": ["auth.check.php"],
        "page_loaded": [],
        "after_request": [],
        "on_request": [],
        "on_import": []
    }
}